syntax = "proto3";
option java_package = "fd.datamodel";
option java_multiple_files = true;

message CustomerTransaction{
  string transId = 1;
  string customerId = 2;
  int64 timestamp = 3;
  int32 amountCents = 4;
}

message CustomerTransactionFraudReport{
  CustomerTransaction trans = 1;
  int32 fraudScore = 2;
}
